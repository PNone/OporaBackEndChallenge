{
  "name": "opora-practice",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --experimental-json-modules src/app.js",
    "run": "webpack serve --entry ./src/app.js --port 8080 --host 0.0.0.0",
    "start": "webpack-dev-server --experimental-json-modules --hot --port 8080 --host 0.0.0.0 --history-api-fallback",
    "startOld": "node src/app.js",
    "build": "webpack -p",
    "clean": "rimraf dist && mkdirp dist"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "module": "^1.2.5",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  }
}
